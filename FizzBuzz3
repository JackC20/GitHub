i = 1 #set counter variable equal to 0

while(i < 101): 
        
    
    if(i % 3 == 0):
        print("Fizz") #if i is evenly divisible by three print Fizz
        
    elif(i % 5 == 0): #if i is evenly divisible by five print Fizz
        print("Buzz")
        
    else:  # #if i is not evenly divisible by 3 or 5  print the number
        print(i)
        
    i += 1 #keep the count going
